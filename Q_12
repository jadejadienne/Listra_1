def main():
    lista_de_numeros = []
    
    for i in range(1, 4):
        numero = int(input("Digite um número: "))
        lista_de_numeros.append(numero)
    
    numeros_impares = []
    for numero in lista_de_numeros:
        if numero % 2 != 0:
            numeros_impares.append(numero)
    
    print("Números ímpares: ", numeros_impares)
    
if __name__ == "__main__":
    main()
