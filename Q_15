def main():
    
    lista_numeros = []
    
    for ler_lista in range(1,6):
        numeros = int(input("digite o numero que voçê deseja: "))
        lista_numeros.append(numeros)
        
    n_menor_q5 = []
    for numeros in lista_numeros:
        if numeros < 5:
         n_menor_q5.append(numeros)
        
    print("numeros menores que cinco: ", n_menor_q5 )
   
    
   
    
   
if __name__ == "__main__":
    main()

