def main():
    
    lista_numeros = []
    
    for ler_lista in range(1,6):
        numeros = int(input("digite o numero que voçê deseja: "))
        lista_numeros.append(numeros)
        
    soma_dos_impares = 0
    numeros_impares = []
    for numeros in lista_numeros:
        if numeros % 2 != 0:
         soma_dos_impares =+ numeros
         numeros_impares.append(numeros)
    
    print("numeros impares: ", numeros_impares)    
    print("soma dos numeros: ", soma_dos_impares)
     
if __name__ == "__main__":
    main()
