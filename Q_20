def main():
    
    lista_numeros = []
    
    for ler_lista in range(1,6):
        numeros = int(input("digite o numero que voçê deseja: "))
        lista_numeros.append(numeros)
        
    lista_numeros.sort()
    
    print("numeros em ordem crecente: ", *lista_numeros, sep = " ")          
     
if __name__ == "__main__":
    main()
