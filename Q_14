def main():
    
    lista_numeros = []
    
    for ler_lista in range(1,4):
        numeros = int(input("digite o numero que voçê deseja: "))
        lista_numeros.append(numeros)
        
    n_maior_q10 = []
    for numeros in lista_numeros:
        if numeros > 10:
         n_maior_q10.append(numeros)
        
    print("numeros maiores que dez: ", n_maior_q10 )  
   
if __name__ == "__main__":
    main()

