def main():
    lista_de_numeros = []
    
    for ler_lista in range(1 , 7):
       numero =  int (input("digite um numero: " ))
       lista_de_numeros.append(numero)
      
    maior_numero_da_lista = lista_de_numeros[0]
    for numero in lista_de_numeros:
        if numero > maior_numero_da_lista:
            maior_numero_da_lista = numero
            
    print("qual Ã© o maior numero da lista: ", maior_numero_da_lista)        
    
if __name__ == "__main__":
 main()    
    
    
  
